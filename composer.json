{
    "name": "datto/json-rpc-serialized",
    "version": "3.0.1",
    "type": "library",
    "description": "Fully unit-tested JSON-RPC 2.0 for PHP",
    "keywords": ["php", "json", "rpc", "jsonrpc", "json-rpc", "php-json-rpc"],
    "homepage": "http://datto.com",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Spencer Mortensen",
            "email": "smortensen@datto.com",
            "homepage": "http://spencermortensen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "jms/serializer": "0.16.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": {
            "Datto\\JsonRpc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datto\\JsonRpc\\Examples\\": "examples/src",
            "Datto\\JsonRpc\\Tests\\": "tests"
        }
    }
